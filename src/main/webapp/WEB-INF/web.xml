<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>ConnectToRoomServlet</servlet-name>
        <servlet-class>servlet.ConnectToRoom</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ConnectToRoomServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WaitServlet</servlet-name>
        <servlet-class>servlet.WaitServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WaitServlet</servlet-name>
        <url-pattern>/wait</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestInteraction</servlet-name>
        <servlet-class>servlet.TestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestInteraction</servlet-name>
        <url-pattern>/testServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AvailableUsersServlet</servlet-name>
        <servlet-class>servlet.AvailableUsersServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AvailableUsersServlet</servlet-name>
        <url-pattern>/availableUsersServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UsernameServlet</servlet-name>
        <servlet-class>servlet.UsernameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UsernameServlet</servlet-name>
        <url-pattern>/usernameServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RemoveRoomServlet</servlet-name>
        <servlet-class>servlet.RemoveRoomServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RemoveRoomServlet</servlet-name>
        <url-pattern>/removeRoomServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StartServlet</servlet-name>
        <servlet-class>servlet.StartServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StartServlet</servlet-name>
        <url-pattern>/startServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PushResultToDBServlet</servlet-name>
        <servlet-class>servlet.PushResultToDBServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PushResultToDBServlet</servlet-name>
        <url-pattern>/pushResultToDBServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OpponentIsGoneServlet</servlet-name>
        <servlet-class>servlet.OpponentIsGoneServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OpponentIsGoneServlet</servlet-name>
        <url-pattern>/opponentIsGoneServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TotalTopServlet</servlet-name>
        <servlet-class>servlet.TotalTopServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TotalTopServlet</servlet-name>
        <url-pattern>/totalTopServlet</url-pattern>
    </servlet-mapping>


</web-app>
